#! /usr/bin/vvp
:ivl_version "10.3 (stable)" "(v10_3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55627f576d20 .scope module, "h_adder_testbench" "h_adder_testbench" 2 3;
 .timescale 0 0;
v0x55627f5886a0_0 .var "a", 0 0;
v0x55627f588760_0 .var "b", 0 0;
v0x55627f588800_0 .net "carry", 0 0, L_0x55627f588b00;  1 drivers
v0x55627f5888d0_0 .net "sum", 0 0, L_0x55627f5889a0;  1 drivers
S_0x55627f576ea0 .scope module, "object" "h_adder" 2 7, 3 1 0, S_0x55627f576d20;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a"
    .port_info 1 /INPUT 1 "b"
    .port_info 2 /OUTPUT 1 "sum"
    .port_info 3 /OUTPUT 1 "carry"
L_0x55627f5889a0 .functor XOR 1, v0x55627f5886a0_0, v0x55627f588760_0, C4<0>, C4<0>;
L_0x55627f588b00 .functor AND 1, v0x55627f5886a0_0, v0x55627f588760_0, C4<1>, C4<1>;
v0x55627f5410e0_0 .net "a", 0 0, v0x55627f5886a0_0;  1 drivers
v0x55627f588400_0 .net "b", 0 0, v0x55627f588760_0;  1 drivers
v0x55627f5884c0_0 .net "carry", 0 0, L_0x55627f588b00;  alias, 1 drivers
v0x55627f588560_0 .net "sum", 0 0, L_0x55627f5889a0;  alias, 1 drivers
    .scope S_0x55627f576d20;
T_0 ;
    %vpi_call 2 11 "$monitor", "a=%b, b=%b, sum=%b, carry=%b", v0x55627f5886a0_0, v0x55627f588760_0, v0x55627f5888d0_0, v0x55627f588800_0 {0 0 0};
    %vpi_call 2 12 "$dumpfile", "h_adder.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55627f576d20 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x55627f5886a0_0;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x55627f588760_0;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x55627f5886a0_0;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x55627f588760_0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x55627f5886a0_0;
    %pushi/vec4 0, 0, 1;
    %cassign/vec4 v0x55627f588760_0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x55627f5886a0_0;
    %pushi/vec4 1, 0, 1;
    %cassign/vec4 v0x55627f588760_0;
    %delay 20, 0;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
